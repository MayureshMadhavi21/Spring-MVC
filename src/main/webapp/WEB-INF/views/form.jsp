	crossorigin="anonymous">

<title>Intercept Example</title>
</head>
<body>
	<div class="container p-5">
		<form action="welcome">
			<div class="form-row align-items-center">
				<div class="col-auto">
					<label class="sr-only" for="inlineFormInput">userName</label> <input
						type="text" name="userName" class="form-control mb-2" id="inlineFormInput"
						placeholder="Enter userName">
				</div>
				<!-- <div class="col-auto">
					<label class="sr-only" for="inlineFormInputGroup">Username</label>
					<div class="input-group mb-2">
						<div class="input-group-prepend">
							<div class="input-group-text">@</div>
						</div>
						<input type="text" class="form-control" id="inlineFormInputGroup"
							placeholder="Username">
					</div>
				</div> -->
				<div class="col-auto">
					<div class="form-check mb-2">
						<input class="form-check-input" type="checkbox"
							id="autoSizingCheck"> <label class="form-check-label"
							for="autoSizingCheck"> Remember me </label>
					</div>
				</div>
				<div class="col-auto">
					<button type="submit" class="btn btn-primary mb-2">Submit</button>
				</div>
			</div>
		</form>

	</div>

	<!-- Optional JavaScript -->
